<?xml version="1.0" encoding="UTF-8" ?>

<Patch>

    <Operation Class="PatchOperationAdd">
        <xpath>Defs/XenotypeDef[defName="Sanguophage"]/doubleXenotypeChances</xpath>
        <value>
            <!-- Not very common because the sanguophage xenotype downgrades most of the regalids' genes. -->
            <!-- The result is a +10 efficiency pawn with very few redeeming factors. -->
            <Turn_XenoType_Regalid>0.01</Turn_XenoType_Regalid>
        </value>
    </Operation>

</Patch>